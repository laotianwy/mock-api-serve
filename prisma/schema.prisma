generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Mock {
  id    Int    @id @default(autoincrement())
  project_name String
  api_name String
  api_method String
  query String
  response_to_mock_struct_data String
}

model User {
  id Int @id @default(autoincrement())
  user_name String
  password String
  nick_name String
}